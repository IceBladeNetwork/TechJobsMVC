<h1>@ViewBag.title</h1>
<h3>@ViewBag.jobs.Count Result(s)</h3>

@for (int i = 0; i < ViewBag.Jobs.Count; i++)
{
    <table class="job-listing">
        @foreach (KeyValuePair<string, string> column in ViewBag.jobs[i])
        {

            <tr>
                <td>@column.Key</td>
                <td><a href="/List/Jobs?column=@column.Key&value=@column.Value">@column.Value</a></td>
            </tr>

        }
    </table>
    <br />
}
